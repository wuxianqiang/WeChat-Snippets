{
  "name": "veturpack",
  "version": "0.1.0",
  "description": "Vuepack adapted for Vetur.",
  "main": "client/index.js",
  "scripts": {
    "build": "webpack --config build/webpack.prod.js",
    "dev": "node build/server.js",
    "lint": "xo **/client/**/*.{vue,js} !node_modules/**"
  },
  "author": "Pine Wu <octref@gmail.com>",
  "license": "MIT",
  "xo": {
    "extends": "./.eslintrc",
    "esnext": true,
    "envs": [
      "browser"
    ]
  },
  "babel": {
    "presets": [
      [
        "vue-app",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-nested": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie > 8"
  ],
  "dependencies": {
    "buefy": "^0.8.16",
    "element-ui": "^2.4.5",
    "lodash": "^4.17.19",
    "quasar": "^1.0.0-beta.0",
    "vue": "^2.6.11",
    "vue-router": "^3.0.1",
    "vuetify": "^2.0.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^1.4.4",
    "@types/lodash": "^4.14.77",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "typescript": "^3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "vetur": {
    "tags": "./tags.json"
  }
}
